#===============================================
# Default targets
# This makefile is only used to clobber specific
# files in the simpl/bin directory
#===============================================
clobber:\
     title
	@-rm fclogger
	@-rm fcshow
	@-rm fcslay
	@-rm tclSurrogate
	@-rm protocolRouter
	@-rm surrogateTcp
	@-rm dumpProtocolTable
	@echo simpl/bin clobber

title:
	@echo simpl/bin clobber starting
